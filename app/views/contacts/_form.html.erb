<div class="forms">
  <h1>Contact Popp Forest Today!</h1>
  <br>
  <%= form_for @contact do |f| %>
    <%= f.label :first_name %>
    <br>
    <%= f.text_field :first_name, required: 'required' %>
    <br><br>
    <%= f.label :last_name %>
    <br>
    <%= f.text_field :last_name, required: 'required' %>
    <br><br>
    <%= f.label :company %>
    <br>
    <%= f.text_field :company %>
    <br><br>
    <%= f.label :email %>
    <br>
    <%= f.text_field :email, required: 'required' %>
    <br><br>
    <%= f.label :phone %>
    <br>
    <%= f.text_field :phone, required: 'required' %>
    <br><br>
    <%= f.label :address %>
    <br>
    <%= f.text_field :address %>
    <br><br>
    <%= f.label :city %>
    <br>
    <%= f.text_field :city %>
    <br><br>
    <%= f.label :zip %>
    <br>
    <%= f.text_field :zip %>
    <br><br>
    <%= f.label :country %>
    <br>
    <%= f.text_field :country %>
    <br><br>
    <%= f.label :message %>
    <br>
    <%= f.text_area :message, required: 'required' %>
    <br>
    <%= f.submit "Submit", class: 'btn btn-primary btn-md' %>
  <% end %>
</div>
